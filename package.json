        "react-loadable": "^5.5.0",
        "react-scripts-ts": "2.17.0"
    },
    "devDependencies": {
        "@types/jest": "^23.3.1",
        "@types/node": "^10.9.1",
        "@types/react": "^16.4.11",
        "@types/react-dom": "^16.0.7",
        "@types/react-fontawesome": "^1.6.4",
        "@types/react-loadable": "^5.4.1",
        "electron": "^2.0.8",
        "npm-run-all": "^4.1.3",
