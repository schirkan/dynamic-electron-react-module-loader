  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*",
      "modules/**/*",
      "build/**/*"
    ]
  },
  "rules": {
    "no-console": false
  },
  "jsRules": {
    "no-console": false    
  }
}